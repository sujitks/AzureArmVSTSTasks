{
  "manifestVersion": 1,
  "id": "summus-azure-vsts-tools",
  "version": "1.1.8",
  "name": "Azure-RM-Tasks",
  "scopes": [
  
  ],
  "description": "Various tasks to help ARM deployment in a vsts pipeline",
  "publisher": "summustech",
  "galleryFlags": [
    "Public"
  ],
  "icons": {
    "default": "static/images/logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "demands": [
    "api-version/3.0"
  ],
  "tags": [
    "Sample"
  ],
  "categories": [
    "Build and release"
  ],
  "screenshots": [
    {
      "path": "static/images/screen1.png"
    }
  ],
  "content": {
    "license": {
      "path": "license.md"
    },

    "details": {
      "path": "overview.md"
    }
  },
  "links": {
    "getstarted": {
      "uri": "https://github.com/sujitks/AzureArmVSTSTasks/wiki"
    },
    "support": {
      "uri": "https://github.com/sujitks/AzureArmVSTSTasks/issues"
    },
    "issues": {
      "uri": "https://github.com/sujitks/AzureArmVSTSTasks/issues"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/sujitks/AzureArmVSTSTasks"
  },
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },
  "files": [
    {
      "path": "azurenewresourceGroup"
    },
    {
      "path": "azurenewstorageaccount"
    },
    {
      "path": "AzureARMTemplateDeployment"
    },
    {
      "path": "AzureAutomationAccount"
    },
    {
      "path": "AzureGetAARegInfo"
    },
    {
      "path": "AzureImportDSCConfiguration"
    },
    {
      "path": "AzureImportAAResource"
    }
  ],
  "contributions": [
    {
      "id": "azurenewresourceGroupTask",
      "type": "ms.vss-distributed-task.tasks",
      "description": "Creates a new Azure resource group",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "Name": "azurenewresourceGroup"
      }
    },
    {
      "id": "AzureArmTemplateDeploymentTask",
      "type": "ms.vss-distributed-task.tasks",
      "description": "Deploys ARM Templates to Azure resource group",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "Name": "AzureARMTemplateDeployment"
      }
    },
    {
      "id": "azurenewstorageaccountTask",
      "type": "ms.vss-distributed-task.tasks",
      "description": "create new storage account Azure resource group",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "Name": "azurenewstorageaccount"
      }
    },
    {
      "id": "AzureAutomationAccountTask",
      "type": "ms.vss-distributed-task.tasks",
      "description": "create new automation account in Azure resource group",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "Name": "AzureAutomationAccount"
      }
    },
    {
      "id": "AzureGetAARegInfoTask",
      "type": "ms.vss-distributed-task.tasks",
      "description": "Get the azure automation account information",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "Name": "AzureGetAARegInfo"
      }
    },
    {
      "id": "AzureImportDSCConfigurationTask",
      "type": "ms.vss-distributed-task.tasks",
      "description": "Import a DSC configuraiton in Azure AA",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "Name": "AzureImportDSCConfiguration"
      }
    },
    {
      "id": "AzureImportAAResourceTask",
      "type": "ms.vss-distributed-task.tasks",
      "description": "Import a DSC resource in Azure AA",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "Name": "AzureImportAAResource"
      }
    }
  ]
}